<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.omys.stickermaker">

    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="com.android.vending.BILLING"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/stickermakerlogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/stickermakerlogo"
        android:supportsRtl="false"
        android:theme="@style/AppTheme"
        tools:node="replace">
        <!--<meta-data android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="@string/admob_ad_id"/>-->
      <!--  <activity
            android:name="com.android.billingclient.api.ProxyBillingActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />-->
     <!--   <activity
            android:name="com.google.android.gms.ads.AdActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"
            android:theme="@android:style/Theme.Translucent" />-->
        <activity
            android:name="com.omys.stickermaker.WhatsAppBasedCode.StickerPackListActivity"
            android:label="@string/title_activity_sticker_packs_list"
            android:configChanges="keyboardHidden|orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.SEND" />
                <data android:mimeType="*/*" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.omys.stickermaker.WhatsAppBasedCode.StickerPackDetailsActivity"
            android:parentActivityName="com.omys.stickermaker.WhatsAppBasedCode.StickerPackListActivity"
            tools:ignore="UnusedAttribute">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.omys.stickermaker.WhatsAppBasedCode.StickerPackListActivity" />
        </activity>
        <activity
            android:name="com.omys.stickermaker.WhatsAppBasedCode.StickerPackInfoActivity"
            android:label="@string/title_activity_sticker_pack_info"
            android:parentActivityName="com.omys.stickermaker.WhatsAppBasedCode.StickerPackDetailsActivity"
            tools:ignore="UnusedAttribute">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.omys.stickermaker.WhatsAppBasedCode.StickerPackDetailsActivity" />
        </activity>

        <activity android:name="com.omys.stickermaker.NewUserIntroActivity"
            android:theme="@style/Theme.Intro"/>

        <provider
            android:name="com.omys.stickermaker.WhatsAppBasedCode.StickerContentProvider"
            android:authorities="${contentProviderAuthority}"
            android:enabled="true"
            android:exported="true"
            android:readPermission="com.whatsapp.sticker.READ">
        </provider>
    </application>

</manifest>